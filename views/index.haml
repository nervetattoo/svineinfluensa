#latest_container
  %h2 Smittede denne m√•neden
  %table#latest.tablesorter
    %thead
      %tr
        %th Fylke
        %th Menn
        %th Kvinner
        %th Ukjent
        %th Total
    %tbody
      - @latest.each do |@d|
        %tr
          %td=@d[:county]
          %td=@d[:male]
          %td=@d[:female]
          %td=@d[:unknown]
          %td=@d[:female] + @d[:male] + @d[:unknown]
    %tfooter
      %tr
        %td Total
        %td=@total[:latest_m]
        %td=@total[:latest_f]
        %td=@total[:latest_u]
        %td=@total[:latest_f] + @total[:latest_m] + @total[:latest_u]
